<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.ezimport" ExtensionBundleVersion="1.0.0"
  ExtensionBundleName="ezimport" Version="8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.ezimport.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="AEFT" Version="[16,99.9]" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="8.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.ezimport.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./public/index-dev.html</MainPath>
          <CEFCommandLine>
            <Parameter>--allow-file-access-from-files</Parameter>
            <Parameter>--allow-file-access</Parameter>
            <Parameter>
              --disable-features=SameSiteByDefaultCookies,CookiesWithoutSameSiteMustBeSecure,NetworkService</Parameter>
            <Parameter>--disable-site-isolation-trials</Parameter>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>EZImport</Menu>
          <Geometry>
            <Size>
              <Height>400</Height>
              <Width>280</Width>
            </Size>
            <MinSize>
              <Height>500</Height>
              <Width>300</Width>
            </MinSize>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>